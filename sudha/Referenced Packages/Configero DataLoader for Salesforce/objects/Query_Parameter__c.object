    <validationRules>
        <fullName>Display_type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval(Display_Type__c, &apos;&apos;)</errorConditionFormula>
        <errorDisplayField>Display_Type__c</errorDisplayField>
        <errorMessage>Display type is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_Source_Type_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>ispickval(List_Source_Type__c, &apos;&apos;) &amp;&amp; (
ispickval(Display_Type__c, &apos;Radio&apos;) ||
ispickval(Display_Type__c, &apos;Single Picklist&apos;) ||
ispickval(Display_Type__c, &apos;Multi Picklist&apos;)
)</errorConditionFormula>
        <errorMessage>List source type is required for selected Display Type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>List_source_required_for_SOQL</fullName>
        <active>true</active>
        <errorConditionFormula>(isblank(List_Source__c) || isblank( List_Value_Field_API_Name__c ) || isblank(List_Label_Field_API_Name__c)) 
&amp;&amp; ispickval(List_Source_Type__c, &apos;SOQL&apos;)</errorConditionFormula>
        <errorMessage>List source (SOQL select statement), label field, and value field are required for List Source Type SOQL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lookup_Object_API_Name_required</fullName>
        <active>true</active>
        <errorConditionFormula>isblank( Object_API_Name__c ) &amp;&amp; ispickval(Display_Type__c , &apos;Lookup&apos;)</errorConditionFormula>
        <errorDisplayField>Object_API_Name__c</errorDisplayField>
        <errorMessage>Lookup Object API Name is required when Display Type is Lookup</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Object_API_required_for_Field_Definition</fullName>
        <active>true</active>
        <errorConditionFormula>(
    isblank( Object_API_Name__c ) ||
    isblank( List_Value_Field_API_Name__c )
)
&amp;&amp; ispickval( List_Source_Type__c , &apos;Field Definition&apos;)</errorConditionFormula>
        <errorMessage>Object API Name and List Value Field API Name are required for List Source Type Field Definition</errorMessage>
    </validationRules>
